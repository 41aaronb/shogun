{
    "Program": "library(shogun)\n\n$program",
    "Statement": "$statement\n",
    "Comment": "#$comment\n",
    "Init": {
        "Construct": "$name <- $type($arguments)",
        "Copy": "$name <- $expr"
    },
    "Assign": "$name <- $expr",
    "Type": {
        "BasicType": "$type",
        "ObjectType": "$type"
    },
    "Expr": {
        "StringLiteral": "'$literal'",
        "BoolLiteral": {
            "True": "TRUE",
            "False": "FALSE"
        },
        "NumberLiteral": "$number",
        "MethodCall": "$object$$$method($arguments)",
        "Identifier": "$identifier"
    },
    "Print": "print($expr)"
}